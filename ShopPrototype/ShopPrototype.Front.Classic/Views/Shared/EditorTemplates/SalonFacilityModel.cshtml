@using ShopPrototype.Modules.Common.Models
@model SalonFacilityModel

<tr>
	<td>@Model.CategoryTitle</td>
	<td>@Model.FacilityTitle @Html.HiddenFor(x => x.FacilityId)</td>
	<td>@Html.CheckBoxFor(x => x.Selected)</td>
	<td>@Html.TextBoxFor(x => x.DurationMin, new { @class = "form-control", @type = "text", placeholder = "Продолжительность в минутах" })</td>
</tr>
